<view class="balance"><block tt:if="{{ifSingle}}"><view class="single_page"><image style="width:100%;" src="{{imgUrl+'/worker/single_page.png'}}" mode="widthFix"></image></view></block><block tt:else><view><u-navbar vue-id="5533c505-1" title="积分管理" autoBack="{{true}}" bgColor="#F7F8FA" titleStyle="color: #010101;font-size: 31rpx;" data-event-opts="{{[['^rightClick',[['rightClick']]]]}}" bind:rightClick="__e" bind:__l="__l" vue-slots="{{['left']}}"><view class="u-nav-slot" style="width:57rpx;height:57rpx;text-align:center;line-height:57rpx;border:1px solid #C5C6CB;border-radius:50%;" slot="left"><u-icon vue-id="{{('5533c505-2')+','+('5533c505-1')}}" name="arrow-left" size="19" color="#4E4E52" custom-style="display: inline-block;line-height: 57rpx;margin:0 auto;left:-2rpx;" bind:__l="__l"></u-icon></view></u-navbar><view class="cont" style="{{'margin-top:'+(marginTop+tabMargin+'px')+';'}}"><view class="top"><view class="top_line"><view class="tit">当前积分</view><view class="count flex"><image style="width:44rpx;margin-right:16rpx;" src="{{imgUrl+'/worker/new/ic_score.png'}}" mode="widthFix"></image><text>{{balance.total_amount}}</text></view></view><view class="middle_line flex flex_btween"><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="right" bindtap="__e">充值积分</view><view data-event-opts="{{[['tap',[['toRule',['$event']]]]]}}" class="left" bindtap="__e"><u-icon vue-id="5533c505-3" name="question-circle" color="#333333" size="22" bind:__l="__l"></u-icon></view></view></view><image style="height:25px;margin-top:15px;" src="{{imgUrl+'/worker/new/score_text_icon.png'}}" mode="heightFix"></image><view class="record"><scroll-view style="{{'height:'+(scrollHeight+'px')+';'}}" scroll-y="true" data-event-opts="{{[['scrolltolower',[['scrolltolower',['$event']]]]]}}" bindscrolltolower="__e"><block tt:for="{{list}}" tt:for-item="item" tt:for-index="index"><block tt:if="{{list.length>0}}"><view class="item flex flex_btween"><view class="left"><view class="remark">{{item.description}}</view><view class="time">{{item.create_time}}</view></view><view class="right num"><text class="{{(item.direction=='income'?'green':'red')}}">{{item.direction=='income'?"+":"-"}}</text>{{''+item.change_amount+''}}</view></view></block></block><block tt:if="{{list.length==0}}"><view class="empty"><image src="{{imgUrl+'/worker/no_data.png'}}" mode="widthFix"></image></view></block></scroll-view></view></view><view hidden="{{!(showMask)}}" class="mask"><view class="inner"><image src="{{imgUrl+'/worker/tx_close.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e"></image><view class="title">积分充值</view><view class="tx_wrap"><view class="tx_inner"><view class="tx_line"><view class="tx_tit">兑换积分</view><view class="input_wrap flex flex_btween"><image style="width:44rpx;" src="{{imgUrl+'/worker/new/ic_score.png'}}" mode="widthFix"></image><input style="padding-left:16rpx;box-sizing:border-box;" type="text" value="10" placeholder="请输入提现金额" disabled="{{true}}"/><text style="font-weight:400;font-size:29rpx;color:#333333;white-space:nowrap;">积分</text></view><view class="tips">积分用于报名精选工作，报名1次扣除2.5个积分</view></view><image src="{{imgUrl+'/worker/new/score_icon.png'}}" mode="widthFix"></image></view></view><view class="tx_btn"><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="btn" bindtap="__e">立即支付 9.9元</view><view data-event-opts="{{[['tap',[['toRule',['$event']]]]]}}" class="tips" bindtap="__e"><text>补充说明</text></view></view></view></view></view></block><my-modal class="vue-ref" vue-id="5533c505-4" data-ref="myModal" bind:__l="__l"></my-modal></view>